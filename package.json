{
  "name": "wingo",
  "version": "1.0.1",
  "description": "Platform to make offers and advertising campaigns in mobiles.",
  "main": "index.js",
  "scripts": {
    "public": "mkdir -p public/js public/css",
    "build-js": "browserify -t [ babelify --presets [ es2015 ] ] app/index.js > public/js/app.js",
    "copy-files": "cp client/css/main.css public/css/app.css && cp client/index.html public/index.html",
    "build": "npm run public && npm run build-js && npm run copy-files",
    "serve": "./node_modules/.bin/babel-node server --presets es2015",
    "start": "npm run build && npm run serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nesslopz/wingo.git"
  },
  "keywords": [
    "offers",
    "discounts",
    "places",
    "awesome",
    "night",
    "people",
    "city",
    "friends",
    "enjoy"
  ],
  "author": "Nestor LÃ³pez <n@wingo.io>",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/nesslopz/wingo/issues"
  },
  "homepage": "https://github.com/nesslopz/inegi-denue#readme",
  "dependencies": {
    "babel-cli": "^6.2.0",
    "babel-core": "^6.2.1",
    "babel-preset-es2015": "^6.1.18",
    "express": "^4.13.3",
    "inegi-denue": "^1.0.4",
    "jquery": "^2.1.4",
    "page": "^1.6.4",
    "browserify": "^12.0.1",
    "serve": "^1.4.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.3.13",
    "babelify": "^7.2.0",
  }
}
